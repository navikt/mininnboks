<?xml version="1.0" encoding="UTF-8"?>
<PolicySet xmlns="urn:oasis:names:tc:xacml:2.0:policy:schema:os"
           PolicySetId="urn:xacml:2.0:nav:henvendelse:policysetid:1"
           PolicyCombiningAlgId="urn:oasis:names:tc:xacml:1.0:policy-combining-algorithm:only-one-applicable">
    <Target/>

    <Policy PolicyId="urn:xacml:2.0:nav:mininnboks:policyid:1"
            RuleCombiningAlgId="urn:oasis:names:tc:xacml:1.0:rule-combining-algorithm:first-applicable">
        <Target>
            <Actions>
                <Action>
                    <ActionMatch MatchId="urn:oasis:names:tc:xacml:1.0:function:string-equal">
                        <AttributeValue DataType="http://www.w3.org/2001/XMLSchema#string">innsending</AttributeValue>
                        <ActionAttributeDesignator DataType="http://www.w3.org/2001/XMLSchema#string" AttributeId="urn:oasis:names:tc:xacml:1.0:action:action-id"/>
                    </ActionMatch>
                </Action>
            </Actions>
        </Target>
        <Rule RuleId="urn:xacml:2.0:nav:mininnboks:ruleid:kode-6-bruker-kan-ikke-bruke-losningen" Effect="Deny">
            <Target>
                <Resources>
                    <Resource>
                        <ResourceMatch MatchId="urn:oasis:names:tc:xacml:1.0:function:string-equal">
                            <AttributeValue DataType="http://www.w3.org/2001/XMLSchema#string">6</AttributeValue>
                            <ResourceAttributeDesignator AttributeId="urn:nav:ikt:tilgangskontroll:xacml:resource:discretion-code" DataType="http://www.w3.org/2001/XMLSchema#string"/>
                        </ResourceMatch>
                    </Resource>
                </Resources>
            </Target>
        </Rule>
        <Rule RuleId="alle-andre" Effect="Permit"/>
    </Policy>
</PolicySet>