                    <version>1.1.9</version>
                </plugin>
            </plugins>
        </pluginManagement>

        <plugins>
            <plugin>
                <groupId>no.nav.maven.plugins</groupId>
                <artifactId>modig-scm-plugin</artifactId>
                <executions>
                    <execution>
                        <id>Check illegal or unwanted dependencies</id>
                        <phase>package</phase>
                        <goals>
                            <goal>failon-unwanted-dependencies</goal>
                            <goal>report-unapproved-dependencies</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.jacoco</groupId>
                <artifactId>jacoco-maven-plugin</artifactId>
                <version>0.6.2.201302030002</version>
                <executions>
                    <execution>
                        <id>jacoco-prepare-agent</id>
                        <goals>
                            <goal>prepare-agent</goal>
                        </goals>
                        <configuration>
                            <propertyName>jacoco.agent.argLine</propertyName>
                        </configuration>
                    </execution>
                    <execution>
                        <id>jacoco-report</id>
                        <phase>prepare-package</phase>
                        <goals>
                            <goal>report</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>no.nav.modig</groupId>
                <artifactId>modig-content-maven-plugin</artifactId>
                <version>1.1.1</version>
                <configuration>
                    <propertiesFilesRoot>${project.basedir}/web/src/main/resources/content</propertiesFilesRoot>
                    <propertiesFiler>
                        <nb>innholdstekster_nb.properties</nb>
                        <nb>sbl-webkomponenter_nb.properties</nb>
                    </propertiesFiler>
                    <menyPunktPaths>
                        <nb>${dialogarena.cms.url}/site/16/minehenvendelser/nb/tekster</nb>
                        <nb>${dialogarena.cms.url}/site/16/sbl-webkomponenter/nb/tekster</nb>
                    </menyPunktPaths>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-enforcer-plugin</artifactId>
                <version>1.2</version>
                <executions>
                    <execution>
                        <id>enforce-dependency</id>
                        <goals>
                            <goal>enforce</goal>
                        </goals>
                        <configuration>
                            <fail>false</fail>
                            <rules>
                                <DependencyConvergence/>
                            </rules>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>
